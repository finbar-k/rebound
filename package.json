{
  "name": "rebound",
  "version": "0.1.0",
  "description": "Intelligent, policy-driven retries for modern distributed systems",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
